<AnchorPane id="clear" prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="boundary.managementTemplateRoom.modifyDeleteTemplateRoom.ModifyDeleteTemplateRoomUI">
    <children>
