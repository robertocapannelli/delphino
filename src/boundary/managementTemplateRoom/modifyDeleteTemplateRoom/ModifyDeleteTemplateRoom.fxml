      <Button layoutX="430.0" layoutY="430.0" mnemonicParsing="false" onAction="#clearInputFields" text="Clear Input fields" AnchorPane.rightAnchor="250.0" />
      <Button fx:id="startBtn" layoutX="620.0" layoutY="37.0" mnemonicParsing="false" onAction="#mainTemplateRoomScene" text="Home page" AnchorPane.rightAnchor="80.0" AnchorPane.topAnchor="37.0" />
