        <Label layoutX="20.0" layoutY="110.0" text="Name Template Room" AnchorPane.leftAnchor="20.0" />
        <ListView fx:id="ListViewTemplateRooms" layoutX="39.0" layoutY="144.0" onMouseClicked="#chooseTemplateRoomAndFillTV" prefHeight="123.0" prefWidth="150.0" AnchorPane.leftAnchor="20.0" />
