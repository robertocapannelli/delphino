      <Label layoutX="320.0" layoutY="150.0" text="Seats" />
      <Label layoutX="320.0" layoutY="190.0" text="Board" />
      <Label layoutX="320.0" layoutY="230.0" text="Projectors" />
      <Label layoutX="320.0" layoutY="270.0" text="Computers" />
      <Label layoutX="320.0" layoutY="310.0" text="Desk" />
      <TextField fx:id="name" disable="true" layoutX="490.0" layoutY="110.0" />
      <TextField fx:id="seats" layoutX="490.0" layoutY="150.0" />
      <TextField fx:id="board" layoutX="490.0" layoutY="190.0" />
